{
  "name": "ignite-gym-pass",
  "version": "0.0.0",
  "description": "GymPass style app.",
  "main": "./build/server.js",
  "author": "Luiz Gon√ßalves<luizhbgoncalves@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "build": "tsup",
    "lint": "eslint . --ext .ts,js",
    "lint:fix": "eslint . --ext .ts,js --fix",
    "test": "vitest run --dir src/use-cases",
    "test:cov": "vitest run --coverage --dir src/use-cases",
    "test:watch": "vitest",
    "test:ui": "vitest --ui --dir src/use-cases",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:ui": "vitest --ui --dir src/http",
    "test:e2e:cov": "vitest run --dir src/http --coverage"
  },
  "dependencies": {
    "@fastify/jwt": "6.7.0",
    "@prisma/client": "4.11.0",
    "bcrypt": "5.1.0",
    "dayjs": "1.11.7",
    "dotenv": "16.0.3",
    "fastify": "4.14.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcrypt": "5.0.0",
    "@types/node": "18.15.0",
    "@vitest/coverage-c8": "0.29.2",
    "@vitest/ui": "0.29.2",
    "eslint": "8.35.0",
    "eslint-plugin-import-helpers": "1.3.1",
    "prisma": "4.11.0",
    "tsup": "6.6.3",
    "tsx": "3.12.3",
    "typescript": "4.9.5",
    "vite-tsconfig-paths": "4.0.5",
    "vitest": "0.29.2",
    "vitest-environment-prisma": "link:./prisma/vitest-environment-prisma"
  }
}
